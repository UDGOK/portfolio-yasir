{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@data/*": ["src/data/*"],
			"@hooks/*": ["src/hooks/*"],
			"@layouts/*": ["src/layouts/*"],
			"@scripts/*": ["src/scripts/*"],
			"@store/*": ["src/store/*"],
			"@styles/*": ["src/styles/*"]
		}
	}
}
